{
    "name": "@digital-boss/n8n-nodes-humhub",
    "version": "0.1.6",
    "description": "A node consuming the HumHub API.",
    "license": "SEE LICENSE IN LICENSE.md",
    "homepage": "https://n8n.io",
    "author": {
        "name": "Valentina Lilova",
        "email": "valentina.lilova@digital-boss.de"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/valentina98/n8n-nodes-humhub.git"
    },
    "main": "index.js",
    "scripts": {
        "dev": "npm run watch",
        "build": "tsc && gulp",
        "tslint": "tslint -p tsconfig.json -c tslint.json",
        "watch": "tsc --watch",
        "test": "jest"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "credentials": [
            "dist/credentials/HumHubApi.credentials.js",
            "dist/credentials/HumHubBasicAuthApi.credentials.js",
            "dist/credentials/HumHubJwtApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/HumHub/HumHub.node.js"
        ]
    },
    "devDependencies": {
        "@types/express": "^4.17.6",
        "@types/jest": "^24.0.18",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/lodash": "^4.14.175",
        "@types/node": "^14.14.40",
        "@types/request-promise-native": "^1.0.17",
        "gulp": "^4.0.0",
        "jest": "^24.9.0",
        "n8n-workflow": "^0.70.0",
        "ts-jest": "^24.0.2",
        "tslint": "^5.17.0",
        "typescript": "~4.3.5"
    },
    "dependencies": {
        "n8n-core": "^0.84.0"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testURL": "http://localhost/",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "testPathIgnorePatterns": [
            "/dist/",
            "/node_modules/"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ]
    }
}
